package basic;
public class InsaImpl implements Insa {
	// 멤버변수가 정의되어 있을때 new해서 받지 않고
	// 1. 생성자를 이용해서 작업
	// 2.
	MemberDAO dao = new MemberDAO();
	@Override
	public void addUser(MemberDTO user) {
		dao.add(user);		
	}

	@Override
	public MemberDTO getUser(String id) {
		dao.getUser("id");
		return null;
	}

}
