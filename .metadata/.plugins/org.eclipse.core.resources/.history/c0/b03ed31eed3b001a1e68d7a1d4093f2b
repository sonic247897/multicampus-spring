package app2;
/*
 * OOP가 적용되어 있는 프로그램
*/
public class MyBeanTest {
	public static void main(String[] args) {
		
		MyBeanStyle obj = new MyBeanStyleB();
		run(obj);
		show(obj);
		
	}
	// 메소드 두 곳에서 같은 객체를 받고 다른 스타일로 사용
	// public static void run(MyBeanStyleA obj)
	public static void run(MyBeanStyleB obj) {
		System.out.println("************************");
		/*obj.testHello("현빈");
		obj.testHello("현빈");*/
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("************************");
	}
	// public static void show(MyBeanStyleA obj)
	public static void show(MyBeanStyleB obj) {
		System.out.println("========================");
		/*obj.testHello("현빈");
		obj.testHello("현빈");*/
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("========================");
	}
	
}
