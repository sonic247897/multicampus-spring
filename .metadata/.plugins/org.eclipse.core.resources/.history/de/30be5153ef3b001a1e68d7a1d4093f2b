package app2;
/*
 * OOP가 적용되어 있는 프로그램
*/
public class MyBeanTest {
	public static void main(String[] args) {
		MyBeanStyle obj = new MyBeanStyleA();
		run(obj);
		show(obj);
		
	}
	// 상위타입으로 매개변수 설정
	public static void run(MyBeanStyle obj) {
		System.out.println("************************");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("************************");
	}

	public static void show(MyBeanStyle obj) {
		System.out.println("========================");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("========================");
	}
	
}
