package basic;
public class InsaImpl implements Insa {
	MemberDAO dao;
	// 0. 쓰지 않는 방법 - 멤버변수(dao)가 정의되어 있을때 new해서 받지 않고
	// dao = new MemberDAO();

	// 1. 생성자를 이용해서 작업
	public InsaImpl(MemberDAO dao) {
		super();
		this.dao = dao;
	}
	
	public InsaImpl() { //기본생성자
		
	}	
	@Override
	public void addUser(MemberDTO user) {
		dao.add(user);		
	}

	@Override
	public MemberDTO getUser(String id) {
		dao.getUser("id");
		return null;
	}

}
