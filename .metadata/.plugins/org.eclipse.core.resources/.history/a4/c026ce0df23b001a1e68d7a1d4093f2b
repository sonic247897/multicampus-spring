package exam01;

public abstract class TV {
	// 1. 전원켜기
	abstract void powerOn();
	abstract void turnOn();
	
	// 2. 전원끄기
	abstract void powerOff();
	abstract void turnOff();
	
	// 3. 소리 올리기
	abstract void volumeUp();
	abstract void soundUp();
	
	// 4. 소리 낮추기
	abstract void volumeDown();
	abstract void soundDown();
}
