package app3;
/*
 * 스프링을 이용해서 프로그램을 실행하기
*/
public class MyBeanTest {
	public static void main(String[] args) {
		//MyBeanStyle obj = new MyBeanStyleA();
		MyBeanStyle obj = new MyBeanStyleB();
		run(obj);
		show(obj);
		
	}
	// 상위타입으로 매개변수 설정했으므로 건드릴 필요x
	public static void run(MyBeanStyle obj) {
		System.out.println("************************");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("************************");
	}

	public static void show(MyBeanStyle obj) {
		System.out.println("========================");
		obj.hello("현빈");
		obj.hello("현빈");
		System.out.println("========================");
	}
	
}
